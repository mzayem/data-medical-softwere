@page
@model data_medical_softwere.Pages.Divisions.CreateModel
@{
     ViewData["Title"] = "Create Division";
}
<h2>Create Division</h2>

<form method="post">
    <div class="form-division">
        <label asp-for="Division.Name"></label>
        <input asp-for="Division.Name" class="form-control" />
        <span asp-validation-for="Division.Name" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>